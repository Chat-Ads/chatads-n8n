{
  "name": "n8n-nodes-chatads",
  "version": "0.1.7",
  "description": "n8n community node for the ChatAds API",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json && cp nodes/ChatAds/chatads.svg dist/nodes/ChatAds/chatads.svg",
    "clean": "rimraf dist",
    "lint": "eslint \"{credentials,nodes}/**/*.ts\"",
    "format": "prettier --write \"{credentials,nodes}/**/*.ts\""
  },
  "keywords": [
    "n8n",
    "chatads",
    "automation",
    "n8n-community-node-package"
  ],
  "author": "ChatAds",
  "repository": {
    "type": "git",
    "url": "https://github.com/Chat-Ads/chatads-n8n"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "n8n-workflow": "^1.33.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "n8n": {
    "credentials": [
      "dist/credentials/ChatAdsApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/ChatAds/ChatAds.node.js"
    ]
  }
}
